<div class="modal-header">
    <button class="close" type="button" ng-click="close()"></button>
    <h3 class="modal-title">{{ 'role_set.edit_modal.header' | trans([], 'role') }}</h3>
</div>
<div class="modal-body" ng-init="init()">
    <div class="row">
        <div class="col-md-12">
            {{ render(controller('CoreUserBundle:AdminManageRole:renderEditRoleSetForm', {'roleSetId': roleSetId})) }}
        </div>
    </div>
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-disabled="isChildFormInvalid()" ng-click="ok()">{{ 'common.modal.button.ok'|trans }}</button>
    <button class="btn default" ng-click="close()">{{ 'common.modal.button.cancel'|trans }}</button>
</div>