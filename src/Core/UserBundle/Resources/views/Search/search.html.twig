<div class="row" ng-init="init({{ 'Core\\UserBundle\\Entity\\UserFriendship' | constants | json_encode() }}, {{ 'Core\\UserBundle\\Entity\\User' | constants | json_encode() }})">
    <div class="col-md-12">
        <div class="board">
            <div class="board-title">
                <div class="caption">
                    <span class="caption-subject">{{ 'result.title'|trans([], 'search') }}</span>
                </div>
            </div>
            <div class="board-body">
                <div class="row">
                    <div class="alert alert-info text-center" ng-if="hasBeenSearched === false">
                        <span class="text text-bold">{{ 'result.no_filters' | trans([], 'search') }}</span>
                    </div>
                    <div class="alert alert-warning text-center" ng-if="hasBeenSearched === true && distributedResult.length === 0">
                        <span class="text text-bold">{{ 'result.no_result' | trans([], 'search') }}</span>
                    </div>
                    <div class="col-md-3 name-card-container" ng-repeat="user in distributedResult[pagination.currentPage]">				
                        <div class="panel panel-default name-card">
                            <div class="panel-body">
                                <div class="row cursor-pointer" ng-click="goToProfile(user.id)">
                                    <div class="col-md-5 text-center">
                                        <span class="name-card-username">{{ '{{ user.username }}' }}</span><br>
                                        <img class="img-circle margin-bottom-5" ng-src="{{ '{{ getProfilePictureSrc(user.id) }}' }}" alt=""/>
                                        <span class="name-card-data">{{ '{{ getGenderString(user.gender) }}' }} ({{ '{{ getAge(user.birthDate) }}' }})</span>
                                    </div>
                                    <div class="col-md-7">	
                                        <span class="name-card-data">{{ '{{ getAddressString(user) }}' }}</span>
                                        <p class="name-card-motto">{{ '{{ user.motto }}' }}</p>
                                    </div>
                                </div>

                                <div class="row margin-top-5">
                                    <div class="col-md-6">
                                        <a ng-if="!((user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_BLOCKED) ||
                                                    (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_BLOCKED) ||
                                                    (user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_ACCEPTED) || 
                                                    (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_ACCEPTED))"
                                           ng-disabled="(user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_PENDING) || (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_PENDING)"
                                           tooltip-enable="(user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_PENDING) || (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_PENDING)" uib-tooltip="{{ 'tooltip.request_pending' | trans([], 'friendship') }}"
                                           class="btn btn-user-action btn-rounded" ng-click="addFriend(user)"><i class="fa fa-user-plus"></i> {{ 'common.user_action.add_friend' | trans([], 'messages') }}
                                        </a>
                                        <span class="name-card-data font-red margin-top-5" ng-if="(user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_BLOCKED) || (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_BLOCKED)">
                                            ({{ 'friendship.status.blocked' | trans([], 'friendship')}})
                                        </span>
                                        <span style="display: inline-block;" class="name-card-data margin-top-10" ng-if="(user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_ACCEPTED) || (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_ACCEPTED)">
                                            ({{ 'friendship.friend' | trans([], 'friendship')}})
                                        </span>
                                    </div>
                                    <div class="col-md-6">
                                        <button ng-if="!((user.requestedStatus && user.requestedStatus === friendshipConstants.STATUS_BLOCKED) || (user.invitedStatus && user.invitedStatus === friendshipConstants.STATUS_BLOCKED))"
                                                class="btn btn-user-action btn-rounded" ng-click="writeMessage(user.id)"><i class="fa fa-comment"></i> {{ 'common.user_action.write_message' | trans([], 'messages') }}
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row text-center" ng-if="pagination.countOfResult > 0">
                    <ul uib-pagination next-text="{{ '{{pagination.nextText}}' }}" previous-text="{{ '{{pagination.previousText}}' }}" total-items="pagination.countOfResult" items-per-page="pagination.resultPerPage" max-size="pagination.maxSize" boundary-link-numbers="pagination.showFirstLast" ng-model="pagination.currentPage" class="pagination"></ul>
                </div>
            </div>
        </div>
    </div>
</div>
