<div class="page-head">
    <div class="page-title">
        <h1>
            {{ '{{ $state.current.data.pageTitle }}' }}
            <small>{{ '{{ pageSubTitle }}' }}</small>
        </h1>
    </div>
</div>
<ul class="page-breadcrumb breadcrumb">
    <li class="active">
        {{ 'breadcrumb.search'|trans([], 'search') }}
    </li>
</ul>
